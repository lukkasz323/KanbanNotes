<Window x:Class="KanbanNotes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KanbanNotes"
        mc:Ignorable="d"
        Title="KanbanNotes" Height="450" Width="800">
    <StackPanel x:Name ="mainPanel" Orientation="Horizontal">
        <StackPanel Style="{StaticResource column}">
            <Border Style="{StaticResource task}"/>
        </StackPanel>
        <StackPanel Style="{StaticResource column}">
            <Border Style="{StaticResource task}"/>
        </StackPanel>
    </StackPanel>
</Window>
